{
  "name": "react-kit",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node bin/run build -e production",
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "node bin/run start",
    "test": "node bin/run test -e test",
    "test:report": "npm test && codecov"
  },
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "normalize.css": "6.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "5.0.3",
    "react-router-dom": "4.0.0",
    "redux": "3.6.0",
    "redux-saga": "0.14.3",
    "reselect": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "6.24.0",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-system-import-transformer": "3.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-regenerator": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "codecov": "2.1.0",
    "commitizen": "2.9.6",
    "css-loader": "0.27.3",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "2.8.0",
    "eslint": "3.18.0",
    "eslint-config-standard": "7.1.0",
    "eslint-config-standard-react": "4.3.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "2.1.1",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "minimist": "1.2.0",
    "node-sass": "4.5.1",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "sass-loader": "6.0.3",
    "sinon": "2.1.0",
    "style-loader": "0.16.0",
    "webpack": "2.3.2",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-dev-server": "2.4.2"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.js$",
    "roots": [
      "src"
    ],
    "modulePaths": [
      "src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/config/**",
      "!**/bin/**",
      "!src/index.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/mock/browser.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/mock/file.js",
      "\\.(css|scss)$": "<rootDir>/config/mock/style.js"
    },
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false
    }
  }
}
