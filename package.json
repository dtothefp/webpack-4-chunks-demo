{
  "name": "react-kit",
  "version": "1.0.0",
  "description": "Ready to go React project",
  "main": "index.js",
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean; npm run build:prod",
    "build:dev": "NODE_ENV=development webpack-cli --config config/webpack.dev.js",
    "build:prod": "NODE_ENV=production webpack-cli --config config/webpack.prod.js",
    "clean": "rimraf dist/ yarn-error.log npm-debug.log",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --open --config config/webpack.dev.js",
    "start:prod": "npm run build && http-server --cors dist/"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bulma": "0.6.2",
    "mini-css-extract-plugin": "0.2.0",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0-beta.21",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-spinkit": "3.0.0",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-dev-server": "3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.9",
    "cssnano": "3.10.0",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "3.0.6",
    "http-server": "0.11.1",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2"
  }
}
